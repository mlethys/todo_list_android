<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/current_projects_container"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:background="@drawable/background3"
    tools:context="pl.mlethys.todolist.view.CcompletedtProjectsActivity"
    tools:ignore="MergeRootFrame" >
    <TextView 
	    android:id="@+id/completed_projects_title"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="@string/completed_projects_title" 
	    android:textSize="30sp"
	    android:layout_gravity="center"/>
    <ListView 
        android:id="@+id/completed_projects_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </ListView>

</LinearLayout>